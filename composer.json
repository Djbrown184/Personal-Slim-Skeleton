{
  "name": "djbrown184/personal-slim-skeleton",
  "description": "A Slim Framework 3 skeleton application for rapid development",
  "keywords": [
    "microframework",
    "rest",
    "router",
    "psr7"
  ],
  "homepage": "http://github.com/Djbrown184/Personal-Slim-Skeleton",
  "license": "MIT",
  "authors": [
    {
      "name": "Djbrown184",
      "email": "offshoresboss@gmail.com",
      "homepage": "http://github.com/Djbrown184/Personal-Slim-Skeleton"
    }
  ],
  "require": {
    "php": ">=5.6",
    "slim/slim": "^3.1",
    "slim/php-view": "^2.0",
    "monolog/monolog": "^1.17",
    "slim/http-cache": "^0.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">=5.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "scripts": {
    "start": "php -S localhost:8080 -t public",
    "test": "phpunit"
  }
}
